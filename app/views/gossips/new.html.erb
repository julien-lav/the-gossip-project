<h1>New gossip</h1>
<p>Find me in app/views/gossips/new.html.erb</p>

<%= form_tag gossips_path do %>
 
  <%= label_tag 'content', 'Content' %>
  <%= text_field_tag 'content' %>
 
  <%= label_tag 'anonymous_author', 'Author' %>
  <%= text_field_tag 'anonymous_author' %>
 
  <%= submit_tag "Add a gossip !"%>
<% end %>